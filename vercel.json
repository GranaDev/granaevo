{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/landingpage.html",
      "destination": "/landingpage.html"
    },
    {
      "source": "/planos.html",
      "destination": "/planos.html"
    },
    {
      "source": "/login.html",
      "destination": "/login.html"
    },
    {
      "source": "/primeiroacesso.html",
      "destination": "/primeiroacesso.html"
    },
    {
      "source": "/dashboard.html",
      "destination": "/dashboard.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}